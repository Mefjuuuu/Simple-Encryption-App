    <TextField fx:id="encryptedTextField" layoutX="115.0" layoutY="28.0" onAction="#handleDecryptClick"/>
    <TextField fx:id="keyField" layoutX="115.0" layoutY="58.0" onAction="#handleDecryptClick"/>
    <TextField fx:id="textField" layoutX="115.0" layoutY="108.0" editable="false"/>
    <Button layoutX="115.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleDecryptClick" text="Decrypt"/>
    <Button layoutX="115.0" layoutY="190.0" fx:id="btnBackToMainMenu" text="Back to main menu!" onAction="#handleBackToMainMenuClick"/>
    <Text layoutX="29.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text to decrypt:"/>
    <Text layoutX="29.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Key:"/>
    <Text layoutX="29.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Decrypted text:"/>
